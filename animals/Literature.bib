@article{Alting.2021,
  title = {The {{Characteristics}} and {{Consequences}} of {{African Wild Dog}} (Lycaon {{Pictus}}) {{Den Site Selection}}},
  author = {Alting, B. F. and Bennitt, E. and Golabek, K. A. and Pitcher, B. J. and McNutt, J. W. and Wilson, A. M. and Bates, H. and Jordan, N. R.},
