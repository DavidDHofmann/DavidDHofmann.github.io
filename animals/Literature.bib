@article{Fortin.2009,
  title = {Group-{{Size-Mediated Habitat Selection}} and {{Group Fusion}}--{{Fission Dynamics}} of {{Bison Under Predation Risk}}},
  author = {Fortin, Daniel and Fortin, Marie-Eve and Beyer, Hawthorne L. and Duchesne, Thierry and Courant, Sabrina and Dancose, Karine},
  year = {2009},
