@article{Kelly.2001,
  title = {Computer-{{Aided Photograph Matching}} in {{Studies Using Individual Identification}}: {{An Example}} from {{Serengeti Cheetahs}}},
  shorttitle = {Computer-{{Aided Photograph Matching}} in {{Studies Using Individual Identification}}},
  author = {Kelly, Marcella J.},
  year = {2001},
