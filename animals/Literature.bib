  keywords = {animal movement,forest canopy closure,Global Positioning System,GPS error,GPS-tracking,snow-tracking,tortuosity},
  file = {/home/david/Zotero/storage/BYLL7E2I/decesare2005.pdf.pdf}
}

@article{DeCesare.2012,
  title = {Transcending {{Scale Dependence}} in {{Identifying Habitat}} with {{Resource Selection Functions}}},
  author = {DeCesare, Nicholas J. and Hebblewhite, Mark and Schmiegelow, Fiona and Hervieux, David and McDermid, Gregory J. and Neufeld, Lalenia and Bradley, Mark and Whittington, Jesse and Smith, Kirby G. and Morgantini, Luigi E. and Wheatley, Matthew and Musiani, Marco},
  year = {2012},
  month = jun,
  journal = {Ecological Applications},
