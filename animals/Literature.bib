  author = {Martins, Vitor Souza and Barbosa, Claudio Clemente Faria and De Carvalho, Lino Augusto Sander and Jorge, Daniel Schaffer Ferreira and Lobo, Felipe De Lucia and Novo, Evlyn M{\'a}rcia Le{\~a}o de Moraes},
  year = {2017},
