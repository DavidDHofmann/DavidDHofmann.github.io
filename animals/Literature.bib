  shorttitle = {Patterns of {{Population Subdivision}}, {{Gene Flow}} and {{Genetic Variability}} in the {{African Wild Dog}} (Lycaon {{Pictus}})},
  author = {Girman, D. J. and Vil{\`a}, C. and Geffen, E. and Creel, S. and Mills, M. G. L. and Mcnutt, J. W. and Ginsberg, J. and Kat, P. W. and Mamiya, K. H. and Wayne, R. K.},
