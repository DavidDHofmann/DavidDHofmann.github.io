@article{Rumiano.2020,
  title = {Remote {{Sensing}} of {{Environmental Drivers Influencing}} the {{Movement Ecology}} of {{Sympatric Wild}} and {{Domestic Ungulates}} in {{Semi-Arid Savannas}}, a {{Review}}},
  author = {Rumiano, Florent and Wielgus, Elodie and Miguel, Eve and {Chamaill{\'e}-Jammes}, Simon and {Valls-Fox}, Hugo and Corn{\'e}lis, Daniel and {Garine-Wichatitsky}, Michel De and Fritz, Herv{\'e} and Caron, Alexandre and Tran, Annelise},
