@article{Rasmussen.2008,
  title = {Achilles' {{Heel}} of {{Sociality Revealed}} by {{Energetic Poverty Trap}} in {{Cursorial Hunters}}.},
  author = {Rasmussen, Gregory~S.~A. and Gusset, Markus and Courchamp, Franck and Macdonald, David~W.},
  year = {2008},
