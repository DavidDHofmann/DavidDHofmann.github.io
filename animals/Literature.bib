@article{Marrec.2020,
  title = {Conceptual {{Framework}} and {{Uncertainty Analysis}} for {{Large-Scale}}, {{Species-Agnostic Modelling}} of {{Landscape Connectivity Across Alberta}}, {{Canada}}},
