@article{Shetty.2021,
  title = {Assessing the {{Effect}} of {{Training Sampling Design}} on the {{Performance}} of {{Machine Learning Classifiers}} for {{Land Cover Mapping Using Multi-Temporal Remote Sensing Data}} and {{Google Earth Engine}}},
  author = {Shetty, Shobitha and Gupta, Prasun Kumar and Belgiu, Mariana and Srivastav, S. K.},
