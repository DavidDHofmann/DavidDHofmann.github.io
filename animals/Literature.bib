@article{Wuesthoff.2021,
  title = {Differential {{Habitat Use}} by {{Sympatric Species}} of {{Mouse Lemurs Across}} a {{Mangrove}}--{{Dry Forest Habitat Gradient}}},
  author = {Wuesthoff, Eric F and Fuller, Todd K and Sutherland, Christopher and Kamilar, Jason M and Ramanankirahina, Rindrahatsarana and Rakotondravony, Romule and Rouse, Sarah and Radespiel, Ute},
