@article{Receveur.2024,
  title = {David {{Versus Goliath}}: {{Early Career Researchers}} in an {{Unethical Publishing System}}},
  author = {Receveur, Aurore and Bonfanti, Jonathan and D'Agata, Stephanie and Helmstetter, Andrew J. and Moore, Nikki A. and Oliveira, Brunno F. and {Petit-Cailleux}, Cathleen and Rievrs Borges, Erica and Schultz, Marieke and Sexton, Aaron N. and Veytia, Devi},
  year = {2024},
