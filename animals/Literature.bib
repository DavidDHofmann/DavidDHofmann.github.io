@article{Morales.2004,
  title = {Extracting {{More Out}} of {{Relocation Data}}: {{Building Movement Models}} as {{Mixtures}} of {{Random Walks}}},
  shorttitle = {Extracting {{More Out}} of {{Relocation Data}}},
  author = {Morales, Juan Manuel and Haydon, Daniel T. and Frair, Jacqui and Holsinger, Kent E. and Fryxell, John M.},
  year = {2004},
