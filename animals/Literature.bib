@article{Davies-Mostert.2015,
  title = {The {{Demography}} and {{Dynamics}} of an {{Expanding}}, {{Managed African Wild Dog Metapopulation}}},
  author = {{Davies-Mostert}, Harriet T. and Mills, Michael G.L. and Macdonald, David W.},
  year = {2015},
