@article{Jongeling.2014,
  title = {The {{Predicament}} of the {{African Wild Dog}}, {{Lycaon}} Pictus, {{Is Less Precarious Than Claimed}}},
  author = {Jongeling, Tjeerd B. and Koetsier, Teun},
  year = {2014},
  journal = {African Journal of Ecology},
  volume = {52},
  number = {4},
  pages = {466--470},
  issn = {1365-2028},
  doi = {10.1111/aje.12142},
  urldate = {2023-11-26},
  abstract = {In a paper published fifteen years ago, Gorman et al. (Nature 391, 479--481) made precise claims about how sensitive the African wild dog is to kleptoparasitism by spotted hyaenas Crocuta crocuta and lions Panthera leo. These claims are regularly referred to in the literature, and so far, they have remained unchallenged. However, careful perusal of their paper and analysis of the available data on energy intake and expenditure by wild dogs show that their claims are unfounded. Contrary to Gorman et al., wild dogs can usually take loss of food by kleptoparasitism in their stride. We present the calculations of the energy budget of wild dogs that remain implicit in the paper by Gorman et al.},
  copyright = {{\copyright} 2014 John Wiley \& Sons Ltd},
  langid = {english},
  keywords = {African wild dog,energy expenditure,Gorman,kleptoparasitism,spotted hyaenas and lions},
